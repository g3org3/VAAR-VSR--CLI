(declare-const total_cpus Int)
(declare-const mem_size Int)
(declare-const vars (Array Int Int))
(assert (= (store vars 0 6) vars))
(assert (= (store vars 1 13) vars))
(assert (= total_cpus (select vars 0)))
(assert (= mem_size (select vars 1)))
(assert (> total_cpus 5))
(assert (< total_cpus 20))
(assert (> mem_size 0))
(assert (< (+ total_cpus mem_size) 20))
